    <item>
        <shape android:shape="oval">
            <size android:height="20dp" android:width="20dp"/>
            <padding android:left="5dp" android:top="5dp" android:right="5dp" android:bottom="5dp"/>
            <stroke android:width="1dp" android:color="@color/never_mix"/>
