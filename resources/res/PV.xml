<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:layout_width="match_parent" android:layout_height="55dp" android:layout_marginTop="10dp">
        <RelativeLayout android:layout_width="match_parent" android:layout_height="match_parent">
            <LinearLayout android:gravity="center" android:orientation="horizontal" android:id="@+id/nexgenarrow" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_marginLeft="15dp">
                <ImageView android:layout_width="40dp" android:layout_height="40dp" android:src="@drawable/mvvm_back"/>
            </LinearLayout>
            <LinearLayout android:orientation="horizontal" android:id="@+id/nexgenlogo" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="15dp" android:layout_toRightOf="@+id/nexgenarrow">
                <ImageView android:layout_width="160dp" android:layout_height="55dp" android:src="@drawable/nextgenv1"/>
            </LinearLayout>
            <LinearLayout android:gravity="center" android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_alignParentRight="true">
                <ImageView android:visibility="gone" android:layout_width="35dp" android:layout_height="35dp" android:layout_marginRight="20dp" android:src="@drawable/nexgenallarm"/>
            </LinearLayout>
        </RelativeLayout>
    </LinearLayout>
    <LinearLayout android:gravity="top|left|center_vertical|center_horizontal|center" android:orientation="vertical" android:background="@color/white" android:layout_width="match_parent" android:layout_height="match_parent">
        <LinearLayout android:orientation="vertical" android:padding="0dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp">
            <LinearLayout android:orientation="horizontal" android:padding="0dp" android:visibility="gone" android:layout_width="match_parent" android:layout_height="120dp" android:layout_marginTop="2dp">
                <LinearLayout android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:orientation="vertical" android:padding="5dp" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_margin="0dp" android:layout_weight="1">
                    <ImageView android:layout_gravity="center" android:layout_width="38dp" android:layout_height="35dp" android:src="@drawable/drive"/>
                    <TextView android:textSize="@dimen/dashboard_txt" android:textStyle="bold" android:textColor="@color/black" android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:minHeight="45dp" android:text="Transport Services" android:fontFamily="sans-serif-light"/>
                </LinearLayout>
                <LinearLayout android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:orientation="vertical" android:padding="5dp" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_margin="0dp" android:layout_weight="1">
                    <ImageView android:layout_width="35dp" android:layout_height="35dp" android:src="@drawable/ic_my_document"/>
                    <TextView android:textSize="@dimen/dashboard_txt" android:textStyle="bold" android:textColor="@color/black" android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:minHeight="45dp" android:text="My Documents" android:fontFamily="sans-serif-light"/>
                </LinearLayout>
                <LinearLayout android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:orientation="vertical" android:padding="5dp" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_margin="0dp" android:layout_weight="1">
                    <ImageView android:layout_width="35dp" android:layout_height="35dp" android:src="@drawable/ic_citizen_sentinel"/>
                    <TextView android:textSize="@dimen/dashboard_txt" android:textStyle="bold" android:textColor="@color/black" android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:minHeight="45dp" android:text="Citizen Sentinel" android:fontFamily="sans-serif-light"/>
                </LinearLayout>
                <LinearLayout android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:orientation="vertical" android:padding="3dp" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_margin="0dp" android:layout_weight="1">
                    <ImageView android:layout_width="35dp" android:layout_height="35dp" android:src="@drawable/ic_informational_services"/>
                    <TextView android:textSize="@dimen/dashboard_txt" android:textStyle="bold" android:textColor="@color/black" android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:minHeight="45dp" android:text="Informational Services" android:fontFamily="sans-serif-light"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content">
                <TextView android:textSize="18sp" android:textStyle="bold" android:textColor="@color/black" android:gravity="left|center_vertical|center_horizontal|center" android:layout_gravity="left|center_vertical|center_horizontal|center" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="20dp" android:layout_marginTop="5dp" android:text="Vehicle Related" android:fontFamily="sans-serif-light"/>
                <TextView android:textSize="16sp" android:textStyle="bold" android:textColor="@color/vehicle_fit" android:gravity="left|center_vertical|center_horizontal|center" android:layout_gravity="left|center_vertical|center_horizontal|center" android:id="@+id/enrolled_state_vahan" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="20dp" android:layout_marginTop="5dp" android:text="Enrolled state" android:fontFamily="sans-serif-light"/>
            </LinearLayout>
            <LinearLayout android:orientation="horizontal" android:padding="0dp" android:layout_width="match_parent" android:layout_height="90dp" android:layout_marginTop="10dp">
                <LinearLayout android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:orientation="vertical" android:id="@+id/pay_your_tax" android:padding="5dp" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_margin="0dp" android:layout_weight="1">
                    <ImageView android:layout_gravity="center" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/mvvm_tax"/>
                    <TextView android:textSize="@dimen/dashboard_txt" android:textStyle="bold" android:textColor="@color/black" android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:minHeight="40dp" android:text="@string/pay_your_tax_" android:fontFamily="sans-serif-light"/>
                </LinearLayout>
                <LinearLayout android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:orientation="vertical" android:id="@+id/d_rc" android:padding="5dp" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_margin="0dp" android:layout_weight="1">
                    <ImageView android:layout_width="35dp" android:layout_height="30dp" android:src="@drawable/duplicate_rc"/>
                    <TextView android:textSize="@dimen/dashboard_txt" android:textStyle="bold" android:textColor="@color/black" android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:minHeight="40dp" android:text="Duplicate RC" android:fontFamily="sans-serif-light"/>
                </LinearLayout>
                <LinearLayout android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:orientation="vertical" android:id="@+id/d_ht" android:padding="5dp" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_margin="0dp" android:layout_weight="1">
                    <ImageView android:layout_width="35dp" android:layout_height="30dp" android:src="@drawable/term"/>
                    <TextView android:textSize="@dimen/dashboard_txt" android:textStyle="bold" android:textColor="@color/black" android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:minHeight="40dp" android:text="Hypothecation Termination" android:fontFamily="sans-serif-light"/>
                </LinearLayout>
                <LinearLayout android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:orientation="vertical" android:id="@+id/change_of_address" android:padding="5dp" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_margin="0dp" android:layout_weight="1">
                    <ImageView android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/mvvm_ic_addres"/>
                    <TextView android:textSize="@dimen/dashboard_txt" android:textStyle="bold" android:textColor="@color/black" android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:minHeight="40dp" android:text="Change of Address" android:fontFamily="sans-serif-light"/>
                </LinearLayout>
                <LinearLayout android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:orientation="vertical" android:id="@+id/d_to" android:padding="5dp" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_margin="0dp" android:layout_weight="1">
                    <ImageView android:layout_gravity="center" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/mvvm_ic"/>
                    <TextView android:textSize="@dimen/dashboard_txt" android:textStyle="bold" android:textColor="@color/black" android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:minHeight="40dp" android:text="Transfer of Ownership" android:fontFamily="sans-serif-light"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:orientation="horizontal" android:padding="0dp" android:layout_width="match_parent" android:layout_height="90dp" android:layout_marginTop="2dp">
                <LinearLayout android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:orientation="vertical" android:padding="5dp" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_margin="0dp" android:layout_weight="1">
                    <ImageView android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/mvvm_ic_addres"/>
                    <TextView android:textSize="@dimen/dashboard_txt" android:textStyle="bold" android:textColor="@color/black" android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:minHeight="40dp" android:text="Change of Address" android:fontFamily="sans-serif-light"/>
                </LinearLayout>
                <LinearLayout android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:orientation="vertical" android:id="@+id/d_ha" android:padding="5dp" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_margin="0dp" android:layout_weight="1">
                    <ImageView android:layout_width="35dp" android:layout_height="30dp" android:src="@drawable/addition"/>
                    <TextView android:textSize="@dimen/dashboard_txt" android:textStyle="bold" android:textColor="@color/black" android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:minHeight="40dp" android:text="Hypothecation Addition" android:fontFamily="sans-serif-light"/>
                </LinearLayout>
                <LinearLayout android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:orientation="vertical" android:id="@+id/hc" android:padding="5dp" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_margin="0dp" android:layout_weight="1">
                    <ImageView android:layout_width="35dp" android:layout_height="30dp" android:src="@drawable/ic_con"/>
                    <TextView android:textSize="@dimen/dashboard_txt" android:textStyle="bold" android:textColor="@color/black" android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:minHeight="40dp" android:text="Hypothecation Continuation" android:fontFamily="sans-serif-light"/>
                </LinearLayout>
                <LinearLayout android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:orientation="vertical" android:id="@+id/noc" android:padding="5dp" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_margin="0dp" android:layout_weight="1">
                    <ImageView android:layout_width="35dp" android:layout_height="30dp" android:src="@drawable/duplicate_rc"/>
                    <TextView android:textSize="@dimen/dashboard_txt" android:textStyle="bold" android:textColor="@color/black" android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:minHeight="40dp" android:text="No Objection Certificate (NOC)" android:fontFamily="sans-serif-light"/>
                </LinearLayout>
                <LinearLayout android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:orientation="vertical" android:id="@+id/rc_partucular" android:padding="5dp" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_margin="0dp" android:layout_weight="1">
                    <ImageView android:layout_width="35dp" android:layout_height="30dp" android:src="@drawable/rc_partucalar"/>
                    <TextView android:textSize="@dimen/dashboard_txt" android:textStyle="bold" android:textColor="@color/black" android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:minHeight="40dp" android:text="RC Particular" android:fontFamily="sans-serif-light"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:orientation="horizontal" android:padding="0dp" android:layout_width="match_parent" android:layout_height="90dp" android:layout_marginTop="2dp">
                <LinearLayout android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:orientation="vertical" android:padding="5dp" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_margin="0dp" android:layout_weight="1">
                    <ImageView android:layout_width="35dp" android:layout_height="30dp" android:src="@drawable/rc_partucalar"/>
                    <TextView android:textSize="@dimen/dashboard_txt" android:textStyle="bold" android:textColor="@color/black" android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:minHeight="40dp" android:text="RC Particular" android:fontFamily="sans-serif-light"/>
                </LinearLayout>
                <LinearLayout android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:orientation="vertical" android:id="@+id/disposeAppLl" android:padding="5dp" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_margin="0dp" android:layout_weight="1">
                    <ImageView android:layout_width="35dp" android:layout_height="30dp" android:src="@drawable/vahan_dispose"/>
                    <TextView android:textSize="@dimen/dashboard_txt" android:textStyle="bold" android:textColor="@color/black" android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:minHeight="40dp" android:text="Dispose Application" android:fontFamily="sans-serif-light"/>
                </LinearLayout>
                <LinearLayout android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:orientation="vertical" android:id="@+id/applStatusLl" android:padding="5dp" android:visibility="visible" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_margin="0dp" android:layout_weight="1">
                    <ImageView android:layout_width="35dp" android:layout_height="30dp" android:src="@drawable/vahan_app_status"/>
                    <TextView android:textSize="@dimen/dashboard_txt" android:textStyle="bold" android:textColor="@color/black" android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:minHeight="40dp" android:text="Application Status" android:fontFamily="sans-serif-light"/>
                </LinearLayout>
                <LinearLayout android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:orientation="vertical" android:id="@+id/mobile_update_service" android:padding="5dp" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_margin="0dp" android:layout_weight="1">
                    <ImageView android:layout_width="35dp" android:layout_height="30dp" android:src="@drawable/smartphone"/>
                    <TextView android:textSize="@dimen/dashboard_txt" android:textStyle="bold" android:textColor="@color/black" android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:minHeight="40dp" android:text="Update Mobile No" android:fontFamily="sans-serif-light"/>
                </LinearLayout>
                <LinearLayout android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:orientation="vertical" android:id="@+id/verifyPaymentLl" android:padding="5dp" android:visibility="invisible" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_margin="0dp" android:layout_weight="1">
                    <ImageView android:layout_width="35dp" android:layout_height="30dp" android:src="@drawable/vahan_verify_payment"/>
                    <TextView android:textSize="@dimen/dashboard_txt" android:textStyle="bold" android:textColor="@color/black" android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:minHeight="40dp" android:text="Verify Payment" android:fontFamily="sans-serif-light"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:orientation="vertical" android:padding="3dp" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_margin="0dp" android:layout_weight="1"/>
    </LinearLayout>
</LinearLayout>
